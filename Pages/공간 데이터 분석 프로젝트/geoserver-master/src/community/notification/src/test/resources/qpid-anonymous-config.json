{
  "name" : "EmbeddedBroker",
  "defaultVirtualHost" : "default",
  "modelVersion" : "6.0",
  "storeVersion" : 1,
  "authenticationproviders" : [ {
    "name" : "anonymous",
    "type" : "Anonymous"
  } ],
  "ports" : [ {
    "name" : "AMQP",
    "port" : "${qpid.amqp_port}",
    "authenticationProvider" : "anonymous",
    "virtualhostaliases": [
        {
          "name": "nameAlias",
          "type": "nameAlias"
        },
        {
          "name": "defaultAlias",
          "type": "defaultAlias"
        },
        {
          "name": "hostnameAlias",
          "type": "hostnameAlias"
        }
      ]
    }
  ],
  "virtualhostnodes": [
    {
      "name": "default",
      "type": "JSON",
      "defaultVirtualHostNode": "true",
      "virtualHostInitialConfiguration": "${qpid.initial_config_virtualhost_config}"
    }
  ]
}