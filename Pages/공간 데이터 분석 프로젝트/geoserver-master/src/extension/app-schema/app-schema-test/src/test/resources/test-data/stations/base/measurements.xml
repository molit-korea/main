<?xml version="1.0" encoding="UTF-8"?>
<as:AppSchemaDataAccess xmlns:as="http://www.geotools.org/app-schema"
                        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xsi:schemaLocation="http://www.geotools.org/app-schema AppSchemaDataAccess.xsd">
  <namespaces>
    <Namespace>
      <prefix>ms_${GML_PREFIX}</prefix>
      <uri>http://www.measurements_${GML_PREFIX}.org/1.0</uri>
    </Namespace>
  </namespaces>
  <sourceDataStores>
    <DataStore>
      <id>measurements_${GML_PREFIX}</id>
      <parameters>
        <Parameter>
          <name>directory</name>
          <value>file:./</value>
        </Parameter>
      </parameters>
    </DataStore>
  </sourceDataStores>
  <targetTypes>
    <FeatureType>
      <schemaUri>measurements_${GML_PREFIX}.xsd</schemaUri>
    </FeatureType>
  </targetTypes>
  <typeMappings>
    <FeatureTypeMapping>
      <sourceDataStore>measurements_${GML_PREFIX}</sourceDataStore>
      <sourceType>measurements_${GML_PREFIX}</sourceType>
      <targetElement>ms_${GML_PREFIX}:Measurement_${GML_PREFIX}</targetElement>
      <attributeMappings>
        <AttributeMapping>
          <targetAttribute>ms_${GML_PREFIX}:Measurement_${GML_PREFIX}</targetAttribute>
          <idExpression>
            <OCQL>ID</OCQL>
          </idExpression>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>ms_${GML_PREFIX}:name</targetAttribute>
          <sourceExpression>
            <OCQL>NAME</OCQL>
          </sourceExpression>
        </AttributeMapping>
        <AttributeMapping>
          <targetAttribute>FEATURE_LINK[1]</targetAttribute>
          <sourceExpression>
            <OCQL>STATION</OCQL>
          </sourceExpression>
        </AttributeMapping>
      </attributeMappings>
    </FeatureTypeMapping>
  </typeMappings>
</as:AppSchemaDataAccess>
