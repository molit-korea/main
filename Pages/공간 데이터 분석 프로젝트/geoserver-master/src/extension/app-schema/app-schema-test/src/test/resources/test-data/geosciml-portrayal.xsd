						</annotation>
					</element>
					<any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ShearDisplacementStructureViewPropertyType">
		<sequence minOccurs="0">
			<element ref="gsmlp:ShearDisplacementStructureView"/>
		</sequence>
	</complexType>
