<?xml version="1.0" encoding="UTF-8"?>
<as:AppSchemaDataAccess xmlns:as="http://www.geotools.org/app-schema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.geotools.org/app-schema AppSchemaDataAccess.xsd">
	<namespaces>
		<Namespace>
			<prefix>gml</prefix>
			<uri>http://www.opengis.net/gml</uri>
		</Namespace>
		<Namespace>
			<prefix>gsml</prefix>
			<uri>urn:cgi:xmlns:CGI:GeoSciML:2.0</uri>
		</Namespace>
		<Namespace>
			<prefix>xlink</prefix>
			<uri>http://www.w3.org/1999/xlink</uri>
		</Namespace>
	</namespaces>	
	
	<sourceDataStores>
		<DataStore>
			<id>postgisng</id>
			<parameters>
				<Parameter>
					<name>dbtype</name>
					<value>${dbtype}</value>
				</Parameter>
				<Parameter>
					<name>host</name>
					<value>${host}</value>					
				</Parameter>
				<Parameter>
					<name>port</name>
					<value>${port}</value>					
				</Parameter>
				<Parameter>
					<name>database</name>
					<value>${database}</value>					
				</Parameter>
				<Parameter>
					<name>user</name>
					<value>${user}</value>					
				</Parameter>
				<Parameter>
					<name>passwd</name>
					<value>${passwd}</value>					
				</Parameter>
				<Parameter>
					<name>schema</name>
					<value>geosciml</value>
				</Parameter>
			</parameters>
		</DataStore>
		<DataStore>
			<id>Oracle</id>
			<parameters>
				<Parameter>
					<name>dbtype</name>
					<value>${dbtype}</value>
				</Parameter>
				<Parameter>
					<name>host</name>
					<value>${host}</value>					
				</Parameter>
				<Parameter>
					<name>port</name>
					<value>${port}</value>					
				</Parameter>
				<Parameter>
					<name>database</name>
					<value>${database}</value>					
				</Parameter>
				<Parameter>
					<name>user</name>
					<value>${user}</value>					
				</Parameter>
				<Parameter>
					<name>passwd</name>
					<value>${passwd}</value>					
				</Parameter>				
			</parameters>
		</DataStore>
	</sourceDataStores>
	<catalog>../../../schemas/catalog.xml</catalog>
	<targetTypes>
		<FeatureType>
			<schemaUri>http://www.geosciml.org/geosciml/2.0/xsd/geosciml.xsd</schemaUri>
		</FeatureType>
	</targetTypes>	 
	<typeMappings>
		<FeatureTypeMapping>			
			<sourceDataStore>${dbtype}</sourceDataStore>
			<sourceType>view_physicaldescription</sourceType>
			<targetElement>gsml:PhysicalDescription</targetElement>
			<attributeMappings>
				<AttributeMapping>
					<targetAttribute>
                        gsml:PhysicalDescription
					</targetAttribute>
					<idExpression>
						<OCQL>id</OCQL>
					</idExpression>
				</AttributeMapping>
				<AttributeMapping>
					<targetAttribute>
                       gsml:density
					</targetAttribute>					
					<sourceExpression>
						<OCQL>id</OCQL>
						<linkElement>if_then_else(isNull(density_value02),'Density_Value02_is_Null','Density_Value02_Not_Null')</linkElement>
						<linkField>FEATURE_LINK</linkField>						
					</sourceExpression>
					<isMultiple>true</isMultiple>	
				</AttributeMapping>	
				<AttributeMapping>
					<targetAttribute>
                       gsml:magneticSusceptibility
					</targetAttribute>					
					<sourceExpression>
						<OCQL>id</OCQL>
						<linkElement>if_then_else(isNull(mag_value02),'magneticSus_Value02_is_Null','magnetscpty_Value02_Not_Null')</linkElement>
						<linkField>FEATURE_LINK</linkField>						
					</sourceExpression>
					<isMultiple>true</isMultiple>	
				</AttributeMapping>	
				<AttributeMapping>
					<targetAttribute>
                       FEATURE_LINK
					</targetAttribute>
					<sourceExpression>
						<OCQL>id</OCQL>						
					</sourceExpression>
				</AttributeMapping>
			</attributeMappings>
		</FeatureTypeMapping>	
		<FeatureTypeMapping>
			<mappingName>Density_Value02_Not_Null</mappingName>
			<sourceDataStore>${dbtype}</sourceDataStore>
			<sourceType>density</sourceType>
			<targetElement>gsml:CGI_NumericRange</targetElement>
			<attributeMappings>	
				<AttributeMapping>
					<targetAttribute>
						gsml:CGI_NumericRange
					</targetAttribute>
					<idExpression>
						<OCQL>id</OCQL>
					</idExpression>
				</AttributeMapping>
				<AttributeMapping>
					<targetAttribute>gsml:lower/gsml:CGI_NumericValue/gsml:principalValue</targetAttribute>
					<sourceExpression>
						<OCQL>value01</OCQL>
					</sourceExpression>
					<ClientProperty>
						<name>uom</name>
						<value>value01_uom</value>
					</ClientProperty> 
				</AttributeMapping>
				<AttributeMapping>
					<targetAttribute>gsml:upper/gsml:CGI_NumericValue/gsml:principalValue</targetAttribute>
					<sourceExpression>
						<OCQL>value02</OCQL>
					</sourceExpression>
					<ClientProperty>
						<name>uom</name>
						<value>value02_uom</value>
					</ClientProperty> 
				</AttributeMapping>
				<AttributeMapping>
					<targetAttribute>
                       FEATURE_LINK
					</targetAttribute>
					<sourceExpression>
						<OCQL>physicaldescription_id</OCQL>						
					</sourceExpression>
				</AttributeMapping>
			</attributeMappings>
		</FeatureTypeMapping>	
		<FeatureTypeMapping>
			<mappingName>Density_Value02_is_Null</mappingName>
			<sourceDataStore>${dbtype}</sourceDataStore>
			<sourceType>density</sourceType>
			<targetElement>gsml:CGI_NumericValue</targetElement>
			<attributeMappings>
				<AttributeMapping>
					<targetAttribute>
						gsml:CGI_NumericValue
					</targetAttribute>
					<idExpression>
						<OCQL>id</OCQL>
					</idExpression>
				</AttributeMapping>
				<AttributeMapping>
					<targetAttribute>gsml:principalValue</targetAttribute>
					<sourceExpression>
						<OCQL>value01</OCQL>
					</sourceExpression>
					<ClientProperty>
						<name>uom</name>
						<value>value01_uom</value>
					</ClientProperty> 
				</AttributeMapping>
				<AttributeMapping>
					<targetAttribute>
                       FEATURE_LINK
					</targetAttribute>
					<sourceExpression>
						<OCQL>physicaldescription_id</OCQL>						
					</sourceExpression>
				</AttributeMapping>
			</attributeMappings>
		</FeatureTypeMapping>
		<FeatureTypeMapping>
			<mappingName>magnetscpty_Value02_Not_Null</mappingName>
			<sourceDataStore>${dbtype}</sourceDataStore>
			<sourceType>magneticsusceptibility</sourceType>
			<targetElement>gsml:CGI_NumericRange</targetElement>
			<attributeMappings>
				<AttributeMapping>
					<targetAttribute>
						gsml:CGI_NumericRange
					</targetAttribute>
					<idExpression>
						<OCQL>id</OCQL>
					</idExpression>
				</AttributeMapping>
				<AttributeMapping>
					<targetAttribute>gsml:lower/gsml:CGI_NumericValue/gsml:principalValue</targetAttribute>
					<sourceExpression>
						<OCQL>value01</OCQL>
					</sourceExpression>
					<ClientProperty>
						<name>uom</name>
						<value>value01_uom</value>
					</ClientProperty> 
				</AttributeMapping>
				<AttributeMapping>
					<targetAttribute>gsml:upper/gsml:CGI_NumericValue/gsml:principalValue</targetAttribute>
					<sourceExpression>
						<OCQL>value02</OCQL>
					</sourceExpression>
					<ClientProperty>
						<name>uom</name>
						<value>value02_uom</value>
					</ClientProperty> 
				</AttributeMapping>
				<AttributeMapping>
					<targetAttribute>
                       FEATURE_LINK
					</targetAttribute>
					<sourceExpression>
						<OCQL>physicaldescription_id</OCQL>						
					</sourceExpression>
				</AttributeMapping>
			</attributeMappings>
		</FeatureTypeMapping>	
		<FeatureTypeMapping>
			<mappingName>magneticSus_Value02_is_Null</mappingName>
			<sourceDataStore>${dbtype}</sourceDataStore>
			<sourceType>magneticsusceptibility</sourceType>
			<targetElement>gsml:CGI_NumericValue</targetElement>
			<attributeMappings>
				<AttributeMapping>
					<targetAttribute>
						gsml:CGI_NumericValue
					</targetAttribute>
					<idExpression>
						<OCQL>id</OCQL>
					</idExpression>
				</AttributeMapping>
				<AttributeMapping>
					<targetAttribute>gsml:principalValue</targetAttribute>
					<sourceExpression>
						<OCQL>value01</OCQL>
					</sourceExpression>
					<ClientProperty>
						<name>uom</name>
						<value>value01_uom</value>
					</ClientProperty> 
				</AttributeMapping>
				<AttributeMapping>
					<targetAttribute>
                       FEATURE_LINK
					</targetAttribute>
					<sourceExpression>
						<OCQL>physicaldescription_id</OCQL>						
					</sourceExpression>
				</AttributeMapping>
			</attributeMappings>
		</FeatureTypeMapping>		
	</typeMappings>
</as:AppSchemaDataAccess>
